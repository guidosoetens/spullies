{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "command": "tsc",
    "args": [
        "--out",
        "main.js",
        "main.ts",
        "--target",
        "es5",
        "--module",
        "amd"
    ], //"args": ["-p", "."],
    "problemMatcher": "$tsc",
    "isBuildCommand": true,
    "group": {
        "kind": "build",
        "isDefault": true
    },
    "esModuleInterop": true,
    "moduleResolution": "node"
}